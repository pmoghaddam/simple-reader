.row
  %aside.three.columns
    = render_cell :feeds, :sidebar
  .nine.columns{role:'content'}
    - if notice
      .alert-box= notice
    - if alert
      .alert-box.alert= alert

    = render 'shared/toolbar'
    = render :partial => 'feed_item', :collection => @feed_items
    = paginate @feed_items