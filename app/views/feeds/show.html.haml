= render 'shared/header'

.row
  %aside.three.columns
    = render_cell :feeds, :sidebar
  .nine.columns{role:'content'}
    - if notice
      .alert-box= notice
    - if alert
      .alert-box.alert= alert

    - @feed_items.each do |feed_item|
      .feed-item
        %h2.title= link_to feed_item.title, feed_item.url
        %p.summary= feed_item.summary
    = paginate @feed_items

= render 'shared/footer'